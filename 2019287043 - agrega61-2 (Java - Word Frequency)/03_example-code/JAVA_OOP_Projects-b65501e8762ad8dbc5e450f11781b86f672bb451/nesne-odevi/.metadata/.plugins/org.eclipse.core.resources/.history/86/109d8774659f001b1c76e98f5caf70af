package itDepartment;

public class PostGraduate extends Graduate {

	private String underGraduateScholl;
	private String underGraduateDepartment;
	public PostGraduate(String underGraduateScholl,String underGraduateDepartment) {
		super();
		// TODO Auto-generated constructor stub
		this.underGraduateDepartment=underGraduateDepartment;
		this.underGraduateScholl=underGraduateScholl;
	}
	@Override
	public String toString() {
		return "PostGraduate [underGraduateScholl=" + underGraduateScholl + ", underGraduateDepartment="
				+ underGraduateDepartment + ", stdNo=" + stdNo + ", stdName=" + stdName + ", stdSirName=" + stdSirName
				+ ", stdDepartment=" + stdDepartment + ", lessonNo=" + lessonNo + ", lessonName=" + lessonName
				+ ", lessonEcts=" + lessonEcts + ", examGrade=" + examGrade + "]";
	}

}
